      url('/static/fonts/lato-v14-latin-regular.svg#Lato') format('svg'); /* Legacy iOS */
    }
    /* lato-700 - latin */
    @font-face {
      font-family: 'Lato';
      font-style: normal;
      font-weight: 700;
      src: url('/static/fonts/lato-v14-latin-700.eot'); /* IE9 Compat Modes */
